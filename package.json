{
  "name": "ha-wechaty",
  "version": "0.4.4",
  "description": "HAWechaty is a Load Balance for providing High Availability for Wechaty Chatbot by spreading requests across multiple WeChat individual accounts.",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "engines": {
    "wechaty": ">=0.38.1"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "example": "ts-node examples/ding-dong-bot.ts",
    "pack": "npm pack",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --noEmit",
    "redux-devtools": "redux-devtools --hostname=localhost --port=8000",
    "start": "ts-node --files examples/ding-dong-bot.ts",
    "redux": "ts-node --files examples/redux-ducks-bot/redux-ducks-bot.ts",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "lint:es": "eslint --ignore-pattern fixtures/ 'src/**/*.ts' 'tests/**/*.ts'"
  },
  "dependencies": {
    "cuid": "^2.1.8",
    "ducks": "^0.10.2",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.5",
    "typesafe-actions": "^5.1.0",
    "wechaty-redux": "^0.2.18"
  },
  "peerDependencies": {
    "wechaty": "*"
  },
  "devDependencies": {
    "@chatie/eslint-config": "^0.8.1",
    "@chatie/git-scripts": "^0.2.5",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.11.1",
    "@types/cuid": "^1.3.0",
    "@types/glob": "^7.1.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/remote-redux-devtools": "^0.5.4",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "pkg-jq": "^0.2.4",
    "redux-devtools-cli": "0.0.1-1",
    "redux-mock-store": "^1.5.4",
    "remote-redux-devtools": "^0.5.16",
    "shx": "^0.3.2",
    "tstest": "^0.4.10",
    "utility-types": "^3.10.0",
    "wechaty": "^0.40.4",
    "wechaty-ducks-contrib": "^0.2.2",
    "wechaty-plugin-contrib": "^0.4.12",
    "wechaty-puppet-mock": "^0.22.30",
    "wechaty-puppet-padplus": "^0.7.20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wechaty/ha-wechaty.git"
  },
  "keywords": [
    "wechaty",
    "ha",
    "cluster",
    "cloud"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wechaty/ha-wechaty/issues"
  },
  "homepage": "https://github.com/wechaty/ha-wechaty#readme",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  }
}
